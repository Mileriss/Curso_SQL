SELECT - Extrai informações de um banco de dados
UPDATE - Atualiza informações em um banco de dados
DELETE - Deleta/exclui informações de um banco de dados
INSERT INTO - Insere novas informações em um banco de dados
CREATE DATABASE - Cria um novo banco de dados
ALTER DATABASE - Modifica um banco de dados
CREATE TABLE - Cria uma nova tabela
ALTER TABLE - Altera/modifica uma tabela
DROP TABLE - Deleta/exclui uma tabela
CREATE INDEX - Cria um index (chave de localização)
DROP INDEX - Deleta/exclui um indice